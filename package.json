{
  "name": "simple-chat",
  "version": "1.0.0",
  "description": "Simple React-based browser chat client using Redux.",
  "main": "index.js",
  "scripts": {
    "serve-client": "react-scripts start",
    "start-server-1": "node server/server-src/socket_connect.js server/server-src/config.json server-1;",
    "start-server-2": "node server/server-src/socket_connect.js server/server-src/config.json server-2;",
    "start-server-3": "node server/server-src/socket_connect.js server/server-src/config.json server-3;",
    "start-server-4": "node server/server-src/socket_connect.js server/server-src/config.json server-4;",
    "install-server": "cd server && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rpartha/simple-chat.git"
  },
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-scripts": "^2.1.3",
    "redux": "^4.0.0",
    "socket.io-client": "^2.2.0"
  },
  "author": "Ramaseshan Parthasarathy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rpartha/simple-chat/issues"
  },
  "homepage": "https://github.com/rpartha/simple-chat#readme",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
